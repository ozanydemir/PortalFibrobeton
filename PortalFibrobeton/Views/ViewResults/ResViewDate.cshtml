                                                    base5 = Convert.ToBase64String(item.ResimT5);
                                                    var r5 = string.Format("data:image/gif;base64,{0}", base5);
                                                    resim5 = r5;
                                                }



                                                var base6 = (string)null;
                                                var resim6 = (string)null;

                                                if (item.ResimT6 != null)
